extends layout

block content
	.w3-container
		.w3-card-4
		header.w3-container.w3-pale-red
			h1 Arquivo de Músicas Digital
			.w3-card-4
		table.w3-table-all
			tr
				th ID
				th Título
				th Nº Partituras
			each o in obras
				tr
					td
						a(href="http://localhost:7024/obra/" + o.id)=o.id
					td= o.tit
					td= o.nparts
		footer.w3-container.w3-pale-red